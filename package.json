  "license": "MIT",
  "homepage": "https://github.com/NormanHuth/laravel-mix-standalone",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "laravel-mix": "^6.0.13",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "vue-template-compiler": "^2.6.12"
  }
}
